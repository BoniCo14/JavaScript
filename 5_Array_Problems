//5) Write a function that has no arguments, and returns an array of the numbers 1 through 10.
let noArgument = () =>
{
    let result = [], i = 1;
   
   while (i<=10)
   {
    result.push(i);
    i++;
   }
   return (result) 
}
console.log(noArgument())

7)) 
// create an array
let myNums = [1, 2, 3, 4, 5];

// create a variable for the sum and initialize it
let sum = 1;
let i=0;

// iterate over each item in the array

while (i<=myNums.length)
{
  sum += myNums[i++];
  i++;
}

console.log(sum) // 15
